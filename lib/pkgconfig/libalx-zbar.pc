Name: libalx-zbar
Description: The libalx C/C++ library (zbar extension)
URL: https://github.com/alejandro-colomar/libalx
Version: <version>
Requires:
Requires.private: zbar libalx-base

prefix=/usr/local
includedir=${prefix}/include
libdir=${prefix}/lib

Cflags: -I${includedir} -D_GNU_SOURCE -D_POSIX_C_SOURCE=200809L
Libs: -L${libdir}/libalx -lalx-zbar
Libs.private:
