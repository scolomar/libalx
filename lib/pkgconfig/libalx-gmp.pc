Name: libalx-gmp
Description: The libalx C/C++ library (GMP extension)
URL: https://github.com/alejandro-colomar/libalx
Version: <version>
Requires: libalx-base
Requires.private:

prefix=/usr/local
includedir=${prefix}/include
libdir=${prefix}/lib

Cflags: -I${includedir} -D_GNU_SOURCE -D_POSIX_C_SOURCE=200809L
Libs: -L${libdir}/libalx -lalx-gmp -lgmp
Libs.private:
