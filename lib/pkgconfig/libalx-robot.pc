Name: libalx-robot
Description: The libalx C/C++ library (Robots extension)
URL: https://github.com/alejandro-colomar/libalx
Version: 1.0~b18
Requires:
Requires.private: libalx-base

prefix=/usr/local
includedir=${prefix}/include
libdir=${prefix}/lib

Cflags: -I${includedir} -D_GNU_SOURCE -D_POSIX_C_SOURCE=200809L
Libs: -L${libdir}/libalx -lalx-robot
Libs.private:
